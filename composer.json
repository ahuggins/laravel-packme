{
  "name": "ptondereau/packme",
  "description": "A starter pack for developing a package with Laravel 5",
  "keywords": ["laravel", "framework", "provider", "package", "starter", "packme"],
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre Tondereau",
      "email": "pierre.tondereau@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ptondereau\\PackMe\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ptondereau\\Tests\\PackMe\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "mnapoli/silly-php-di": "^1.0",
    "constant-null/backstubber": "^0.4.0",
    "symfony/filesystem": "^3.1"
  },
  "require-dev": {
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": ">=4.8.24"
  },
  "bin": [
    "packme"
  ]
}
